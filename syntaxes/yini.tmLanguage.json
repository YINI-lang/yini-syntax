{
  "name": "YINI",
  "scopeName": "source.yini",
  "patterns": [
    {
      "name": "comment.line.shebang.yini",
      "match": "^#!.*$"
    },
    {
      "name": "comment.line.double-slash.yini",
      "match": "//.*$"
    },
    {
      "name": "comment.line.hash.yini",
      "match": "#[ \\t]+.*$"
    },
    {
      "name": "comment.line.semicolon.yini",
      "match": "^[ \\t]*;.*$"
    },    
    {
      "name": "comment.block.yini",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "meta.line.disabled.yini",
      "match": "--.*$"
    },
    {
      "name": "constant.language.boolean.yini",
      "match": "(?i)\\b(true|false|on|off|yes|no)\\b"
    },
    {
      "name": "constant.language.null.yini",
      "match": "(?i)\\bnull\\b"
    },
    {
      "name": "keyword.control.yini",
      "match": "^(?i)@yini\\b"
    },    
    {
      "name": "entity.name.type.class",
      "match": "^(\\s*)(\\^{1,})(\\s*)([A-Za-z0-9_\\-]+)"
    },
    {
      "name": "variable.other.key.yini",
      "match": "^\\s*([A-Za-z0-9_\\-]+)(?=\\s*=)"
    },
    {
      "name": "constant.numeric.hex.yini",
      "match": "(0[xX][0-9A-Fa-f]+|#[0-9A-Fa-f]+)"
    },
    {
      "name": "constant.numeric.yini",
      "match": "(?<=\\=|:|,|\\[|\\{)\\s*([+-]?\\d+(?:\\.\\d+)?(?:[eE][+-]?\\d+)?)"
    },
    {
      "name": "string.quoted.single.yini",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "name": "constant.character.escape.yini",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "string.quoted.double.yini",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.yini",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "punctuation.separator.key-value.yini",
      "match": "\\s*=\\s*"
    },
    {
      "name": "punctuation.separator.colon.yini",
      "match": "\\s*:\\s*"
    },
    {
      "name": "punctuation.separator.comma.yini",
      "match": ","
    },
    {
      "name": "punctuation.section.brackets.begin.yini",
      "match": "\\["
    },
    {
      "name": "punctuation.section.brackets.end.yini",
      "match": "\\]"
    },
    {
      "name": "punctuation.section.braces.begin.yini",
      "match": "\\{"
    },
    {
      "name": "punctuation.section.braces.end.yini",
      "match": "\\}"
    }
  ],
  "repository": {},
  "fileTypes": [
    "yini", "Yini", "YINI"
  ],
  "uuid": "3a3c46a3-f5b0-4c4a-b7e8-0a48c2a1ecbf"
}
