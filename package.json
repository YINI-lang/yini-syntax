{
  "name": "yini-syntax",
  "displayName": "YINI Syntax Highlighting",
  "description": "Syntax highlighting for the YINI config files (works in VSCode and other editors supporting TextMate grammars).",
  "version": "1.0.0",
  "publisher": "local",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "yini",
        "aliases": ["YINI", "yini"],
        "extensions": [".yini", ".Yini", ".YINI"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yini",
        "scopeName": "source.yini",
        "path": "./syntaxes/yini.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YINI-lang/yini-syntax"
  },
  "homepage": "https://github.com/YINI-lang/yini-syntax#readme",
  "bugs": { "url": "https://github.com/YINI-lang/yini-syntax/issues" }
}
